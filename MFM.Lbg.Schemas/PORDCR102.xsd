            <xsd:documentation>Foreign Trade: Export/Import: Header Data</xsd:documentation>
           </xsd:annotation>
           <xsd:complexType>
            <xsd:sequence>
             <xsd:element name="TRANSPORT_MODE" minOccurs="0">
